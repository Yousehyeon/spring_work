package aop_p.webToon;

import org.springframework.stereotype.Component;

@Component
public class Fishing {
	
	public String fishing(String aa) {
		return "낚시시작";
	}
	
	public String baitx(String bb) {
		return "미끼를 넣으세요";
	}

	public String sbait(String cc, int dd) {
		System.out.println("풀어주자");
		return "현재 물고기는" null;
	}
	
	
	
	
}
