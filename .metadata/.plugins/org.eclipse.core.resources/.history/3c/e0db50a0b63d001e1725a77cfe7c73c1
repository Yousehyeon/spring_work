package aaa.model;

import java.util.ArrayList;

import lombok.Data;

@Data
public class OrderData {

	OrderAddr ad;
	ArrayList<OrderProduct> arr;
	int sum;
	
	public int getSum() {
		for(int i=0; i<arr.size(); i++) {
			sum += arr.get(i).getTotal();
			
		}
		return sum;
		for(OrderData i : arr)
	}
	
	
}
