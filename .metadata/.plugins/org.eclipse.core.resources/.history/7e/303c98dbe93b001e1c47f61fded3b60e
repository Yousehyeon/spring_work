package aop_p.webToon;

import org.springframework.stereotype.Component;

@Component
public class Fishing {
	
	public String fishing(String aa, String bb) {
		System.out.println("낚시 시작:"+aa+","+bb);
		return "낚시시작";
	}
	
	public String baitx() {
		return "미끼를 넣으세요";
	}

	public String sbait() {
		System.out.println("풀어주자");
		return null;
	}
	
	
	
	
}
