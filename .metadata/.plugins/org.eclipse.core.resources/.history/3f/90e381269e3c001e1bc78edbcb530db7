package aop_p.adv;

import org.aspectj.lang.JoinPoint;
import org.springframework.stereotype.Component;

@Component
public class StudentAdvice {

	void ab(JoinPoint joinPoint) {
		System.out.println("ab:"+joinPoint.getSignature().toShortString());
	}
	
	void cd(JoinPoint joinPoint) {
		System.out.println("cdv:"+joinPoint.getSignature().toShortString());
	}
	
	void ef(JoinPoint joinPoint) {
		System.out.println("qw(int, String):"+joinPoint.getSignature().toShortString()+ "=>12, 아기상어");
	}

}
