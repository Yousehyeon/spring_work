package aop_p.webToon;

import org.springframework.stereotype.Component;

public class Fish {
    private String name;
    private int size;
      
    public Fish() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Fish(String name, int size) {
        this.name = name;
        this.size = size;
    }
    
    public String getName() {
        return name;
    }
    
    public int getSize() {
        return size;
    }
}