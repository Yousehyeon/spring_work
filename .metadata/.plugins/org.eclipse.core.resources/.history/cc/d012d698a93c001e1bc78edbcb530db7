package aop_p.adv;

import org.aspectj.lang.JoinPoint;
import org.springframework.stereotype.Component;

@Component
public class StudentAdvice {

	void ab(JoinPoint joinPoint, int aa, int bb, int cc) {
		System.out.println("뭐라하는 반 점수:"+aa+","+bb+","+cc);
	}
	
	void cd(JoinPoint joinPoint, int dd, int ee, int ff, int gg) {
		System.out.println("일반 반 점수:"+ dd+"," +ee+","+ff+","+gg);
	}
	
	void ef(JoinPoint joinPoint, int hh, int ii, int jj, int kk, int ll) {
		System.out.println("예체능 반 점수:"+hh+","+ii+","+jj+","+kk+","+ll);
	}
	void vg(JoinPoint joinPointl) {
		System.out.println("예체능 반 점수:"+hh+","+ii+","+jj+","+kk+","+ll);
	}
	
	
}
