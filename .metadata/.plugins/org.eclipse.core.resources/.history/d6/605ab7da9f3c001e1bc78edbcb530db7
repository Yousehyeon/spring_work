package aop_p.adv;

import org.aspectj.lang.JoinPoint;
import org.springframework.stereotype.Component;

@Component
public class StudentAdvice {

	void ab(JoinPoint joinPoint, int aa, String bb) {
		System.out.println("ab(int, String):"+joinPoint.getSignature().toShortString());
	}
	
	void cd(JoinPoint joinPoint) {
		System.out.println("cd:"+joinPoint.getSignature().toShortString());
	}
	
	void ef(JoinPoint joinPoint, int ee) {
		System.out.println("ef(int):"+joinPoint.getSignature().toShortString());
	}

}
