package aop_p.webToon;

import org.springframework.stereotype.Component;

@Component
public class Student {
	
	public int subject2(int aa, int bb, int cc) {
		int sum = aa + bb + cc;
		int avg = sum/3;
		System.out.println("뭐라하는 반 총점 = "+sum);
		System.out.println("뭐라하는 반 평균 = "+avg);
		return avg;
	}
	public int subject3(int dd, int ee, int ff, int gg) {
		int sum1 = dd + ee + ff + gg;
		int avg1 = sum1/4;
		System.out.println("일반 반 점수 총합 = "+sum1);
		System.out.println("일반 반 점수 평균 = "+avg1);
		return avg1;
	}
	public int subject4(int hh, int ii, int jj, int kk, int ll) {
		int sum2 = hh + ii + jj + kk + ll;
		int avg2 = sum2/5;
		System.out.println("예체능 반 점수 총합 = "+sum2);
		System.out.println("예체능 반 점수 평균 = "+avg2);
		return avg2;
	}
	
	
}
