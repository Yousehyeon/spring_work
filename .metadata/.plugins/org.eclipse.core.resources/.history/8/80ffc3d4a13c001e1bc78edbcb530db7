package aop_p.adv;

import org.aspectj.lang.JoinPoint;
import org.springframework.stereotype.Component;

@Component
public class StudentAdvice {

	void ab(JoinPoint joinPoint, int aa, int bb, int cc) {
		System.out.println("ab(int, int, int):"+aa+bb+cc);
	}
	
	void cd(JoinPoint joinPoint, int dd, int ee, int ff, int gg) {
		System.out.println("cd(int, int, int, int, int):"+ dd +ee+ff+gg);
	}
	
	void ef(JoinPoint joinPoint, int hh, int ii, int jj, int kk, int ll) {
		System.out.println("ef(int):"+joinPoint.getSignature().toShortString());
	}
	
	
}
